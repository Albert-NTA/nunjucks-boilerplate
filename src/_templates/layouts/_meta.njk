<head>
    <meta charset="utf-8">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    {% if baseRoot %}
        <base href="../">
    {% endif %}
    <title>
        {% if pageTitle %}
            {{pageTitle}}
        {% endif %}
    </title>
    <style>
        [data-js="true"] {
            display: none;
        }
    </style>
    <link href="//cdn.jsdelivr.net/npm/bootstrap@3.3.7/dist/css/bootstrap.min.css" rel="stylesheet"/>
    <link href="//cdn.jsdelivr.net/npm/bootstrap-slider@10.6.1/dist/css/bootstrap-slider.css" rel="stylesheet"/>
    <link href="//fonts.googleapis.com/css?family=Roboto:400,500,600|Space+Mono" rel="stylesheet">
    <link href="//cdnjs.cloudflare.com/ajax/libs/ionicons/4.4.1/css/ionicons.min.css" rel="stylesheet"/>
    <link href="//cdn.jsdelivr.net/npm/codemirror@5.48.4/lib/codemirror.css" rel="stylesheet"/>
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/codemirror@5.48.4/theme/xq-light.css">
    <script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    {% for css in mainCSS %}
        <link href="assets/css/{{css}}.css" rel="stylesheet"/>
    {% endfor %}
    {% for css in extraCSS %}
        <link href="assets/css/{{css}}.css" rel="stylesheet"/>
    {% endfor %}
    <script>
        window.dataQueries = [];
    </script>
</head>
