{% extends "layout.njk"%}
{% set pageTitle = 'Home | Got It Pro On-demand Excel help' %}
{% set navbar = {
    context: general.navbar.context,
    logined: home.navbar.logined
    }
%}
{% block pageHeader %}
    {% include "component/_gi-navBar.njk" %}
{% endblock %}

{% block pageBody %}
    <div class="gi-settingPage u-flexGrow-1">
        <div
            class="gi-settingPage-Inner">
            <!-- expert home: -->
            <div class="container">
                <!-- system notification: -->

                <!-- main content: -->
                    <div class="u-flex u-width-100 u-paddingVertical-3"> <div class="u-flexGrow-1 u-paddingRight-3">
                        <div class="u-backgroundWhite u-border">
                            <div class="gi-settingPage-profile">
                                <div class="u-positionRelative u-padding-3 u-textCenter">
                                    <img src="assets/images/profile-empty-icon.png" class=" u-marginTop-2  u-roundedCircle u-ImgFluid" width="96" alt=""/>
                                    <div class="u-textExtraLarge u-fontMedium u-marginBottom-2">Henry</div>
                                    {# <div class="u-textGray">1824</div> #}
                                    <div class="u-textGray u-marginBottom-1">Latest Activity On 03/08/2019</div>
                                    <div class="u-textPrimary u-textLarge">Good Morning!</div>
                                </div>
                            </div>
                            <div class="u-table u-width-100 u-paddingHorizontal-3 u-paddingBottom-3">
                                <div class="u-tableRow u-backgroundLightalt u-textCenter u-textGray">
                                    <div class="u-tableCell u-padding-1" style="border-right: 1px solid #fff;">Earnings</div>
                                    <div class="u-tableCell u-padding-1" style="border-left: 1px solid #fff;">Average</div>
                                </div>
                                <div class="u-tableRow u-textCenter">
                                    <div class="u-tableCell">
                                        <div class="value">
                                            <span>104.25</span>
